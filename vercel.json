{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "includeFiles": [
    "dist/**",
    "node_modules/pg/**",
    "node_modules/pg-hstore/**"
  ]
}
